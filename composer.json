{
    "name": "thoughtco/statamic-stache-sqlite",
    "autoload": {
        "psr-4": {
            "Thoughtco\\StatamicStacheSqlite\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.1",
        "pixelfear/composer-dist-plugin": "^0.1.5",
        "statamic/cms": "^5.0",
        "ryangjchandler/orbit": "^1.4",
        "spatie/laravel-ray": "^1.40"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^8.0",
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^2.24",
        "phpunit/phpunit": "^10.0"
    },
    "extra": {
        "statamic": {
            "name": "Statamic Stache Sqlite",
            "description": "Live in the future and use Sqlite for the stache."
        },
        "laravel": {
            "providers": [
                "Thoughtco\\StatamicStacheSqlite\\ServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "pixelfear/composer-dist-plugin": true
        }
    }
}
